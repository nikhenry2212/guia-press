<%- include('../../partials/header') %>
    <%- include('../../partials/navbar') %>


        <div class="container">
            <hr>
            <h2>Usuários</h2>
            <a href="/admin/users/create" class="btn btn-success">Criar novo usuário</a>
            <hr>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>id</th>
                        <th>email</th>
                    </tr>
                </thead>
                <tbody>
                    <%users.forEach(user => { %>
                        <tr>
                            <th>
                                <%= user.id %>
                            </th>
                            <th>
                                <%= user.email %>
                            </th>

                        </tr>
                        <% })%>
                </tbody>
            </table>
        </div>

        <%- include('../../partials/footer') %>